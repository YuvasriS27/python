a=2
b=4


list1=[]
list2=[]
list3=[]

for i in range(1,a+1):
    if(a%i==0):
        list1.append(i)
for j in range(1,b+1):
    if(b%j==0):
        list2.append(j)

for k in list1:
    if k in list2:
        list3.append(k)

for l in list3:
    if(list3[0]<=l):
        continue
print(l)


        
        
